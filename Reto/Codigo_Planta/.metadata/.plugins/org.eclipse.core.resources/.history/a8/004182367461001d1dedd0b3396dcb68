#include "stm32h7xx_hal.h"

#include <stdio.h>
#include <complex.h>

typedef float complex cplx;

void print_int(char *string, int var);

void print_float(char *string, float var);

void print_clpx(char *string, cplx var);

void print_arrayInt(int arr);

void print_arrayFloat(float arr);

void print_arrayCplx(cplx arr);
