#ifndef HSEM_ID_0
#define HSEM_ID_0 (0U) /* HW semaphore 0*/
#endif

#define MT 128			// Muestras totales (128 Hz)
#define KP 0.344
#define KI 0.0112
/* USER CODE END PD */

